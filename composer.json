{
  "name": "outerweb/filament-image-library",
  "description": "This package adds a way to interact with outerweb/image-library in Filament.",
  "homepage": "https://github.com/outer-web/filament-image-library",
  "license": "MIT",
  "authors": [
    {
      "name": "Outerweb",
      "email": "info@outerweb.be"
    }
  ],
  "require": {
    "php": "^8.0",
    "filament/filament": "^3.2",
    "filament/spatie-laravel-translatable-plugin": "^3.2",
    "laravel/framework": "^10.0|^11.0",
    "outerweb/image-library": "^2.4",
    "outerweb/filament-translatable-fields": "^1.0|^2.0",
    "spatie/laravel-package-tools": "^1.16"
  },
  "autoload": {
    "psr-4": {
      "Outerweb\\FilamentImageLibrary\\": "src/"
    },
    "files": [
      "src\\Helpers\\helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outerweb\\FilamentImageLibrary\\FilamentImageLibraryServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
